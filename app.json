{
  "expo": {
    "name": "Gaming Zone",
    "slug": "gaming-zone",
    "version": "1.0.2",
    "icon": "./assets/logo-background.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-background.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#0c1a33"
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0c1a33"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yh.gamingzone",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-4635812020796700~2053599689"
      }
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#0c1a33",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new interactions"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },

    "plugins": [
      "expo-localization",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4635812020796700~2053599689",
          "iosAppId": "ca-app-pub-4635812020796700~1466759291"
        }
      ]
    ]
  }
}
